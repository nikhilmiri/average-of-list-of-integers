import java.util.ArrayList;
public class ExampleList {
  public static void main(String[] args) {
    ArrayList<Integer> li = new ArrayList<Integer>();
    li.add(29);
    li.add(20);
    li.add(15);
    li.add(40);
    li.add(70);
    int total = 0;
    int avg;
    for(int i = 0; i < li.size(); i++)
        total += li.get(i);
        avg = total / li.size();
        System.out.println("The Average is: " + avg);
  }
}
